{
    "version": 2,
    "builds": [
      {
        "src": "api/instagramScraper.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/instagramScraper",
        "dest": "api/instagramScraper.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/instagramScraper"
      }
    ]
  }
  